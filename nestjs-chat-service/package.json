  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "nodemon --exec ts-node src/main.ts",
    "start:debug": "nodemon --exec ts-node --inspect=9229 src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "tsc --noEmit",
